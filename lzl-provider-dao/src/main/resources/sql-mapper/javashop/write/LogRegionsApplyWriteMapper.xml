<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lzl.dao.javashop.write.LogRegionsApplyWriteDao">
	
	<insert id="post">
		insert into log_regions_apply (member_id,city,address_info,latitude,longitude,create_time )
		values (#{memberId},#{city},#{addressInfo},#{latitude},#{longitude},#{createTime})
	</insert>

    <update id="put">
        update log_regions_apply
        <set>
				member_id = #{memberId},
				city = #{city},
				address_info = #{addressInfo},
				latitude = #{latitude},
				longitude = #{longitude},
				create_time = #{createTime},
        </set>
        where  
    </update>

</mapper>