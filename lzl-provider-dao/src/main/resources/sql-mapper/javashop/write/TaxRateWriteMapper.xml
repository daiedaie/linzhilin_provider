<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ejavashop.dao.shop.write.provider.TaxRateWriteDao">
	
	<delete id="deleteTaxRateById" parameterType="java.lang.Integer">
		delete from tax_rate
		where tax_rate_id = #{taxRateId}
	</delete>
	
	<insert id="post">
		insert into tax_rate (tax_rate_id,tax_rate_name,rate,create_time,modify_time )
		values (#{taxRateId},#{taxRateName},#{rate},#{createTime},#{modifyTime})
	</insert>
	
	 <update id="put">
        update tax_rate
        <set>
				tax_rate_id = #{taxRateId},
				tax_rate_name = #{taxRateName},
				rate = #{rate},
				create_time = #{createTime},
				modify_time = #{modifyTime},
        </set>
        where tax_rate_id = #{taxRateId} 
    </update>

</mapper>